<main class="container">
  <div class="row text-center justify-content-center">
    <div class="col">
      <h1>Forms</h1>
      <%= link_to 'New Form', new_form_path %>
      <ul>
        <% @forms.each do |form| %>
          <li><%= link_to form.title, form %></li>
        <% end %>
      </ul>
    </div>
  </div>
</main>
